    <consensusAmbiguityMinReads>2</consensusAmbiguityMinReads>
    <consensusAmbiguityMinProportion>0.10</consensusAmbiguityMinProportion>
    <consensusProduceAmbiguityCodes>true</consensusProduceAmbiguityCodes>
</samReporter>
