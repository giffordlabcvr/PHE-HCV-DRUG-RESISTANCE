<ecmaFunctionInvoker>
	<scriptFileName>phdrReportingController.js</scriptFileName>
	<function>
		<name>reportFastaWeb</name>
		<consumesBinary>true</consumesBinary>
		<parameter>
			<name>base64</name>
		</parameter>
		<parameter>
			<name>fastaFilePath</name>
		</parameter>
		<documentResultType/> 
	</function>
	
	<function>
		<name>reportFasta</name>
		<parameter>
			<name>fastaFilePath</name>
			<pathCompleter/>
		</parameter>
		<documentResultType/> 
	</function>
	<function>
		<name>reportBam</name>
		<parameter>
			<name>bamFilePath</name>
			<pathCompleter/>
		</parameter>
		<documentResultType/> 
	</function>
	<function>
		<name>reportFastaAsHtml</name>
		<parameter>
			<name>fastaFilePath</name>
			<pathCompleter/>
		</parameter>
		<parameter>
			<name>htmlFilePath</name>
			<pathCompleter/>
		</parameter>
		<okFromNullResultType/> 
	</function>
	<function>
		<name>reportBamAsHtml</name>
		<parameter>
			<name>bamFilePath</name>
			<pathCompleter/>
		</parameter>
		<parameter>
			<name>htmlFilePath</name>
			<pathCompleter/>
		</parameter>
		<okFromNullResultType/> 
	</function>
	<configDocument name="phdrSamThresholds">
		<minDepth glueType="Integer">10</minDepth>
		<minMapQ glueType="Integer">5</minMapQ>
		<minQScore glueType="Integer">5</minQScore>
		<minReadProportionPct glueType="Double">15.0</minReadProportionPct>
	</configDocument>

</ecmaFunctionInvoker>